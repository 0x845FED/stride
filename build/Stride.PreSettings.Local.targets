<!-- Build file pre-included by all Stride projects -->
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <StrideCommonPreSettingsName>Stride</StrideCommonPreSettingsName>
    <StridePlatforms Condition="'$(StridePlatforms)' == ''">Windows</StridePlatforms>
    <StrideGraphicsApisTestsWindows Condition="'$(StrideGraphicsApisTestsWindows)' == ''">Vulkan</StrideGraphicsApisTestsWindows>
    <StrideGraphicsApisWindows Condition="'$(StrideGraphicsApisWindows)' == ''">Direct3D11;Vulkan</StrideGraphicsApisWindows>
  </PropertyGroup>
</Project>

