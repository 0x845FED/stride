<!-- Build file pre-included by all Stride projects -->
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <StrideCommonPreSettingsName>Stride</StrideCommonPreSettingsName>
    <StridePlatforms Condition="'$(StridePlatforms)' == ''">Windows</StridePlatforms>
  </PropertyGroup>
  <PropertyGroup Condition="'$(StridePlatform)' == 'Windows'">
    <StrideGraphicsApis Condition="'$(StrideGraphicsApis)' == ''">Direct3D11;Direct3D12</StrideGraphicsApis>
    <StrideGraphicsApiDependentBuildAll>true</StrideGraphicsApiDependentBuildAll>
  </PropertyGroup>
</Project>
